<article data-area="{{ item.tags[0]|lower }}" class="item col-xs-12 col-sm-6 col-md-3" data-id="{{ item.id }}">
    <div class="wrapper">
        <h2><a href="{{ item.url }}" target="_blank" title="Gå til {{ item.name }}'s website">{{ item.name }}</a></h2>
        <address><a href="https://www.google.dk/maps/place?q={{ item.name|url_encode(true) }},{{ item.address|url_encode(true) }},dk" target="_blank" data-toggle="modal" data-target="#myModal" title="{{ item.name }} på Google maps">{{ item.address|replace({', ':'<br>'})|raw }}</a></address>
        <div class="credit"><a href="{{ path('view', {'id' : item.id}) }}">Mere info</a></div>
    </div>
</article>

