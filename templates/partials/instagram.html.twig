{% for item in feed %}
{% if loop.first %}
<h3 class="container-fluid">Sidste {{ loop.length }} snaps p√• Instagram</h3>
<section class="container-fluid social instagram">
{% endif %}
    <article class="item col-md-6">
        <div class="wrapper">
            <img src="{{ item.image.url }}">
            <p>
                <time><a href="{{ item.link }}">{{ item.created_time|date('Y-m-d H:i') }}</a></time>
                {{ item.caption|linkify('instagram')|nl2br }}
            </p>
        </div>
    </article>
{% if loop.last %}
</section>
{% endif %}
{% endfor %}
